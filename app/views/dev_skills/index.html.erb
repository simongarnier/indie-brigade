<p id="notice"><%= notice %></p>

<h1>Listing Dev Skills</h1>

<table>
  <thead>
    <tr>
      <th>Dev</th>
      <th>Skill</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dev_skills.each do |dev_skill| %>
      <tr>
        <td><%= dev_skill.dev %></td>
        <td><%= dev_skill.skill %></td>
        <td><%= link_to 'Show', dev_skill %></td>
        <td><%= link_to 'Edit', edit_dev_skill_path(dev_skill) %></td>
        <td><%= link_to 'Destroy', dev_skill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dev skill', new_dev_skill_path %>
