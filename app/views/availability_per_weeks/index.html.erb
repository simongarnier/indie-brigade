<p id="notice"><%= notice %></p>

<h1>Listing Availability Per Weeks</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @availability_per_weeks.each do |availability_per_week| %>
      <tr>
        <td><%= availability_per_week.name %></td>
        <td><%= link_to 'Show', availability_per_week %></td>
        <td><%= link_to 'Edit', edit_availability_per_week_path(availability_per_week) %></td>
        <td><%= link_to 'Destroy', availability_per_week, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Availability per week', new_availability_per_week_path %>
