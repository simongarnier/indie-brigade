<%= render 'partials/menu' %>
<div class="container">
	<div class="row">
		<div class="col col-sm-12 signup-form box-content">
			<h1>Sign up</h1>
			<section id="sign">
			  <%= form_for user do |form| %>
			    <%= render partial: '/users/form', object: form %>
					<%= recaptcha_tags %>
			    <div class="div-btn">
			    	<button class="signup btn-cta" type="submit">Sign Up</button>
			    </div>
			  <% end %>
			</section>
		</div>
	</div>
</div>
